{
  "name": "uploader",
  "version": "1.0.0",
  "description": "System for generating file upload links. ",
  "main": "index.ts",
  "scripts": {
    "build:server": "tsc -p ./tsconfig.json",
    "build:client:dev": "webpack --mode=development",
    "build:client:rel": "webpack ",
    "build": "npm run build:server | npm run build:client:dev ",
    "release": "npm run build:server | npm run build:client:rel",
    "test": "npm run test:server | npm run test:client",
    "test:server": "mocha ./test/server",
    "test:client": "testcafe chrome ./test/ui/*",
    "lint": "eslint ./client ./server",
    "forever": "forever stopall &&  npm run release && forever start build/server/index.js",
    "nodemon": "nodemon ./build/server/index.js",
    "start": "node ./build/server/index.js"
  },
  "keywords": [
    "file",
    "sharing"
  ],
  "author": "Isabela",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "2.302.0",
    "axios": "0.18.0",
    "body-parser": "1.18.3",
    "bootstrap": "4.1.1",
    "bootstrap-vue": "2.0.0-rc.11",
    "cors": "2.8.4",
    "cron": "1.3.0",
    "crypto-js": "3.1.9-1",
    "es6-shim": "0.35.3",
    "eslint": "5.0.1",
    "express": "4.16.3",
    "express-jwt": "5.3.1",
    "font-awesome": "4.7.0",
    "font-roboto": "0.0.2",
    "jsonwebtoken": "8.3.0",
    "mkdirp": "0.5.1",
    "moment": "2.22.2",
    "mongoose": "5.1.7",
    "multer": "1.3.1",
    "testcafe": "0.21.1",
    "typescript": "3.0.1",
    "uglify-es": "3.3.9",
    "vue": "2.5.17",
    "vue-clip": "1.0.0",
    "vue-clipboard2": "0.2.0",
    "vue-router": "3.0.1",
    "vue2-dropzone": "3.2.2",
    "vuejs-paginate": "2.0.1"
  },
  "devDependencies": {
    "@types/body-parser": "1.17.0",
    "@types/es6-promise": "3.3.0",
    "@types/es6-shim": "0.31.37",
    "@types/express": "4.16.0",
    "@types/jsonwebtoken": "7.2.8",
    "@types/mongodb": "3.1.2",
    "@types/mongoose": "5.2.2",
    "@types/node": "10.5.3",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "chai": "4.1.2",
    "css-loader": "1.0.0",
    "file-loader": "1.1.11",
    "fork-ts-checker-webpack-plugin": "0.4.4",
    "nodemon": "1.17.5",
    "pug": "2.0.3",
    "pug-loader": "2.4.0",
    "pug-plain-loader": "1.0.0",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.2",
    "ts-loader": "4.4.2",
    "vue-class-component": "6.2.0",
    "vue-loader": "15.2.6",
    "vue-property-decorator": "7.0.0",
    "vue-template-compiler": "2.5.17",
    "webpack": "4.16.3",
    "webpack-cli": "3.1.0"
  }
